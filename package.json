{
  "name": "m-fs",
  "version": "1.0.2",
  "description": "Promise version of common Node.js fs methods",
  "main": "dist/lib/main.js",
  "types": "dist/lib/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/m-fs",
  "bugs": "https://github.com/mgenware/m-fs/issues",
  "keywords": [
    "fs",
    "promise"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/m-fs.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run copy && yarn run dev_test",
    "dev_test": "jest",
    "build": "yarn test",
    "compile": "tsc",
    "dev": "yarn run clean && tsc -w",
    "prepublishOnly": "yarn run test",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "copy": "copyfiles \"tests/data/**/*.*\" \"dist\"",
    "clean:dist": "rm -rf dist",
    "clean": "npm run clean:dist"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "mkdirp-promise": "^5.0.1",
    "node-filter-async": "^0.0.3"
  },
  "files": [
    "dist/lib",
    ".gitignore",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.4",
    "copyfiles": "^1.2.0",
    "jest": "^22.2.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
